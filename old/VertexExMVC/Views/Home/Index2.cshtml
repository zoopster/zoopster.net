@Html.ExpressSection(new SectionModel() { HasContainerAndRow = true, Height = SectionHeight.Height4, Src = "/Content/img/mix/services-bg.jpg" },
    @<text>
        <div class="col-md-12  text-white text-center">
            <h5 class="text-color-secondary">ARE YOU READY?</h5>
