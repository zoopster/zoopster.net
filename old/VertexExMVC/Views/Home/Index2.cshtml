@helper RenderCarousel()
{
    @Html.ExpressCarousel(new CarouselModel() { DisplayCount = 6, UIProps = { Style = CarouselStyle.DotsInside, Margin = 50 } },
    @<text> <img alt="" class="img-fluid" src="~/Content/img/clients/amazon-original.svg"> </text>,
    @<text> <img alt="" class="img-fluid" src="~/Content/img/clients/google-original.svg"> </text>,
    @<text> <img alt="" class="img-fluid" src="~/Content/img/clients/lenovo-original.svg"> </text>,
    @<text> <img alt="" class="img-fluid" src="~/Content/img/clients/spotify-original.svg"> </text>,
    @<text> <img alt="" class="img-fluid" src="~/Content/img/clients/shopify-original.svg"> </text>,
