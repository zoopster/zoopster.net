                    @Html.ExpressSectionSubTitle("Express uses valid and clean code so that you make sure that Google and other <br /> search engines will love it.")

                    @Html.ExpressSpace()
                </div>
            </div>
        </div>

        <div class="container-fluid">
            <div class="row pr-9 pl-9">
                <div class="col-md-12">
                    @{
                        var portfolioModel = PortfolioData.GridData;
                        portfolioModel.PageSize = 8;
                        portfolioModel.UIProps.ColumnCount = 4;
                        portfolioModel.UIProps.ShowFilter = false;
                        portfolioModel.UIProps.ShowPaginations = false;
                    }
                    @Html.ExpressPortfolio(portfolioModel)

                </div>
            </div>

            <div class="row">
                <div class="col-md-12 text-center">
                    @Html.ExpressSpace()

                    <a class="btn btn-dark btn-rounded" href="#"><span class="text"><i class="fa fa-eye ml-0"></i> View Our Works</span></a>
                    <button type="button" class="btn btn-outline-primary btn-rounded"><i class="fa fa-eye ml-0"></i> See Our Features</button>
                </div>
            </div>
        </div>
    </text>)


