@Html.ExpressSection(new SectionModel() { HasContainerAndRow = true, Height = SectionHeight.Height5, ClassName = "gr-bg-1" },
    @<text>
        <div class="col-md-5">
            <h4 class="text-5 text-white">Check Our Skils</h4>
            @Html.ExpressSectionTitle("Our Skils", TextColor.White)

            @Html.ExpressSectionSubTitle(
           new TextElement("Express uses valid and clean code so that you <br /> make sure that search engines will love it.")
           {
               TextColor = TextColor.White,
               Opacity = Opacity.O_5,
               TextSize = TextSize.Text_4,
               FontWeight = FontWeight.Light
           })

            <button type="button" class="btn btn-primary btn-rounded shadow-5x">See More</button>
        </div>

        <div class="col-md-7 text-white">
            @Html.ExpressProgressBar(new ProgressBarModel() { Value = 25, Label = new TextElement("Javascript", TextColor.White), UIProps = { IsLabelOutside = true } })
            @Html.ExpressProgressBar(new ProgressBarModel() { Value = 45, Label = new TextElement("Photoshop", TextColor.White), UIProps = { IsLabelOutside = true } })
            @Html.ExpressProgressBar(new ProgressBarModel() { Value = 65, Label = new TextElement("Bootstrap", TextColor.White), UIProps = { IsLabelOutside = true } })
            @Html.ExpressProgressBar(new ProgressBarModel() { Value = 75, Label = new TextElement("HTML/CSS", TextColor.White), UIProps = { IsLabelOutside = true } })
            @Html.ExpressProgressBar(new ProgressBarModel() { Value = 95, Label = new TextElement("Javascript", TextColor.White), UIProps = { IsLabelOutside = true } })
        </div>
    </text>)

@Html.ExpressSection(new SectionModel() { Height = SectionHeight.Height4 },
    @<text>
